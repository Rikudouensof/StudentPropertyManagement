@model StudentPropertyManagement.Models.Accomodation

@{
  ViewData["Title"] = "Accomodation Details";
  Layout = "~/Views/Shared/_DashbordLayout.cshtml";

}

<div class="row">
  <div class="col-lg-8 col-xlg-9 col-md-12">
    <div class="card">
      <div class="card-body">



        <div>
          <h4>Accomodation</h4>
          <hr />
          <dl class="row">
            <dt class="col-sm-2">
              @Html.DisplayNameFor(model => model.Name)
            </dt>
            <dd class="col-sm-10">
              @Html.DisplayFor(model => model.Name)
            </dd>
            <dt class="col-sm-2">
              @Html.DisplayNameFor(model => model.ExpiryDate)
            </dt>
            <dd class="col-sm-10">
              @Html.DisplayFor(model => model.ExpiryDate)
            </dd>
            <dt class="col-sm-2">
              @Html.DisplayNameFor(model => model.JoinedDate)
            </dt>
            <dd class="col-sm-10">
              @Html.DisplayFor(model => model.JoinedDate)
            </dd>
            <dt class="col-sm-2">
              @Html.DisplayNameFor(model => model.AccomodationPosition)
            </dt>
            <dd class="col-sm-10">
              @Html.DisplayFor(model => model.AccomodationPosition)
            </dd>
            <dt class="col-sm-2">
              @Html.DisplayNameFor(model => model.Student)
            </dt>
            <dd class="col-sm-10">
              @Html.DisplayFor(model => model.Student.Id)
            </dd>
            <dt class="col-sm-2">
              @Html.DisplayNameFor(model => model.Space)
            </dt>
            <dd class="col-sm-10">
              @Html.DisplayFor(model => model.Space.Id)
            </dd>
          </dl>
        </div>
        <div>
          <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
          <a asp-action="Index">Back to List</a>
        </div>
      </div>
    </div>
  </div>
</div>